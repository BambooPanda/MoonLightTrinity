      a.Centaur
      h2 BRADWARDEN - The Centaur Warchief
      ul
        .kiri
          li
            img.icon(align='left', src='images/Hero STR/Sentinel/Centaur Warchief/Centaur Warchief.jpg')
            .character
              img(src='images/Hero STR/Sentinel/Centaur Warchief/character.gif')
